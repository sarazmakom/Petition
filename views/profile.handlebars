<h1>Set your profile by adding the city, age and the url to your webpage</h1>

{{#if error}}
<div class="error">Oops! Something went wrong. Please try again.</div>
{{/if}}

<form method="POST" action='/profile'>
    <h2>city:</h2>
<input type="city" name="city" placeholder="city">

<h2>age:</h2>
<input type="age" name="age" placeholder="age">

<h2>url:</h2>
<input type="url" name="url" placeholder="url">

<input type='hidden' name='_csrf' value="{{csrfToken}}">

<button type="submit">Continue</button>

</form>
